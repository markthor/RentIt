@{
    ViewBag.Title = "ChannelList";
}

@model List<RentItMvc.Models.Channel>

<h1>List of channels</h1>
<div class="row-fluid"> 
        <table class="table table-hover">
            <tr>  
                <th>Name</th>  
                <th>Description</th>  
                <th>Rating</th>  
                <th>Hits</th>  
              </tr> 
            @foreach (var p in Model)
            {
                <tr>
                    <td>
                        @p.Name
                    </td>
                    <td>
                        @p.Description
                    </td>  
                    <td>
                        @p.Rating
                    </td>  
                    <td>
                        @p.Hits
                    </td>  
                </tr>
            }
        </table>
</div>
