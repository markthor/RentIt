@model List<RentItMvc.Models.GuiTrack>

<div style="width: 100%; max-height: 350px; overflow-x: hidden; overflow-y: auto">
    <table class="table table-hover">
        <tr>
            <th>Artist</th>
            <th>Title</th>
        </tr>
        @foreach (var t in Model)
        { 
            <tr>
                <td>
                    <div class="text-overflow" style="margin-top: 2px; width: 250px" title="@t.ArtistName">
                        @t.ArtistName
                    </div>
                </td>
                <td>
                    <div class="text-overflow" style="margin-top: 2px; width: 500px"  title="@t.TrackName">
                        @t.TrackName
                    </div>
                </td>
            </tr>
        }
    </table>
</div>